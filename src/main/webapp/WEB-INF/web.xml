<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>SvLogin</servlet-name>
    <servlet-class>com.vtc.controlador.SvLogin</servlet-class>
  </servlet>

  <servlet-mapping>
      <servlet-name>SvLogin</servlet-name>
      <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SvConvenio</servlet-name>
    <servlet-class>com.vtc.controlador.SvConvenio</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SvConvenio</servlet-name>
    <url-pattern>/convenios</url-pattern>
  </servlet-mapping>
</web-app>